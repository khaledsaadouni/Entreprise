@{
    ViewData["Title"] = "Stock";
}

<section class="d-flex align-items-center justify-content-center">
    <div class="container" data-aos="fade-up">

        <section id="portfolio" class="portfolio">
            <div class="container" data-aos="fade-up">

                <div class="section-title">
                    <h2 style="font-size: xx-large;color:black">Stock</h2>
                </div>
                
                    @foreach (Stock s in @Model)
                    {

                        <div class="icon-box mt-5 mt-lg-0" data-aos="zoom-in" data-aos-delay="70">
                            <i class="bx bx-@ViewData[(string?)s.Name]"></i>
                            <h4>@Html.ActionLink(s.Name, "AllProduct", "Stock",new {id =s.Id })</h4>
                        </div>

                    }
             
            </div>
        </section>

    </div>
</section>